      id="aal2_plugin"
      title="AAL2 Authentication Plugin"
      class="c2.pas.aal2.plugin.AAL2Plugin"
      >
    <!-- Activate plugin for these interfaces -->
    <interface id="IAuthenticationPlugin" />
    <interface id="IExtractionPlugin" />
    <interface id="IChallengePlugin" />
    <interface id="ICredentialsResetPlugin" />
  </plugin>
</pas>
